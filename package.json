{
  "name": "rota",
  "version": "0.0.2",
  "description": "Routing with reinforcement learning",
  "main": "js/main.js",
  "scripts": {
    "build": "browserify . -o build/bundle.js",
    "watch": "watchify . --full-paths -o build/bundle.js -dv",
    "serve": "open http://localhost:1442 && python -m SimpleHTTPServer 1442",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rolyatmax/rota.git"
  },
  "keywords": [
    "routing",
    "reinforcement",
    "learning"
  ],
  "author": "Taylor Baldwin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rolyatmax/rota/issues"
  },
  "homepage": "https://github.com/rolyatmax/rota",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "hbsify": "1.0.1",
    "standard": "^10.0.2",
    "watchify": "3.7.0"
  },
  "dependencies": {
    "cat-rom-spline": "1.0.0",
    "gl-vec2": "^1.0.0",
    "regl": "^1.3.0",
    "underscore": "1.7.0"
  },
  "standard": {
    "ignore": [
      "build",
      "js/lib"
    ]
  },
  "browserify": {
    "transform": [
      "hbsify",
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ]
    ]
  }
}
